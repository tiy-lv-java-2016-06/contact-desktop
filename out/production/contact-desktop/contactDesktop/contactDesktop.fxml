<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="contactDesktop.Controller">
   <children>
      <HBox prefHeight="33.0" prefWidth="600.0">
         <children>
            <TextField id="Name" fx:id="nameText" promptText="Name" HBox.hgrow="SOMETIMES" />
            <TextField id="PhoneNumber" fx:id="phoneText" promptText="Phone #" HBox.hgrow="SOMETIMES" />
            <TextField id="Email" fx:id="emailText" promptText="Email" HBox.hgrow="SOMETIMES" />
            <Button mnemonicParsing="false" onAction="#addContact" prefHeight="27.0" prefWidth="63.0" text="Add" HBox.hgrow="ALWAYS" />
            <Button mnemonicParsing="false" onAction="#removeContact" prefHeight="27.0" prefWidth="77.0" text="Remove" />
         </children>
      </HBox>
      <ListView fx:id="list" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
   </children>
</VBox>
